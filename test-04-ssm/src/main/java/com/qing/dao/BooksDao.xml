<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qing.dao.BooksDao">
    <select id="queryAll" resultType="Books">
        select
            id,book_name as bookName,book_price as bookPrice,book_details as bookDetails
         from
            t_book;
    </select>
</mapper>